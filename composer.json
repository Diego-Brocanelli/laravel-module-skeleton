{
    "name": "bnw/package-skeleton",
    "description": "Um esqueleto para pacotes composer desenvolvido pela Bueno Networks.",
    "keywords": [
        "bnw",
        "skeleton",
        "laravel"
    ],
    "homepage": "https://github.com/bueno-networks/package-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Pereira",
            "email": "ricardo.pereira@buenonetworks.com.br",
            "homepage": "https://www.buenonetworks.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^7.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "league/flysystem": ">=1.0.8",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bnw\\Skeleton\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnw\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Bnw\\Skeleton\\SkeletonFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bnw\\Skeleton\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
